{
    "name": "joselfonseca/laravel-tactician",
    "description": "Laravel implementation of the Tactician Command Bus",
    "keywords": [
      "laravel",
      "laravel-tactician",
      "Command Bus",
      "Tactician"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/tactician": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery" : "^1.0",
        "orchestra/testbench": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Joselfonseca\\LaravelTactician\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joselfonseca\\LaravelTactician\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
      "laravel": {
        "providers": [
          "Joselfonseca\\LaravelTactician\\Providers\\LaravelTacticianServiceProvider"
        ]
      }
    }
}
